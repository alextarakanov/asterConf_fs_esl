#this is dockerFile only for create own Image for my scripts.
# pip freeze - make requirements.txt file

FROM python:3.8.3-buster
WORKDIR /usr/src/app
COPY requirements.txt ./
RUN apt-get update -y && apt-get install -y libc6-i386 lib32stdc++6 && pip install --no-cache-dir -r requirements.txt
